generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

enum Role {
  super_admin
  store_admin
  store_manager
  cashier
}
model User {
  id Int @id @default(autoincrement())
  name String
  email String? @unique
  role Role
  password String
  createdAt  DateTime   @default(now())
  updatedAt  DateTime   @updatedAt
  // profile Profile?
  // profileId Int @unique
}

